<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PastHistoryActivity">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:text="지난 이용 내역"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="269dp"
        android:layout_height="84dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:text="날짜 : 2019년 1월 5일\n사용시간 : 3시간\n기종 : SM3\n번호 52가1234  "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />


    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="200dp"
        android:fillViewport="true"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9">

        <LinearLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textview_frontal1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="전면 상단"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_frontal1"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/frontal1_pic" />

            <TextView
                android:id="@+id/textview_frontal2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="전면"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_frontal2"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/frontal2_pic" />

            <TextView
                android:id="@+id/textview_profile1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="운전자석 앞면"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_profile1"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/profile1_pic" />

            <TextView
                android:id="@+id/textview_profile2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="운전자석 뒷면"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_profile2"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/profile2_pic" />

            <TextView
                android:id="@+id/textview_profile3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="보조석 뒷면"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_profile3"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/profile3_pic" />

            <TextView
                android:id="@+id/textview_profile4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="보조석 앞면"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_profile4"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/profile4_pic" />

            <TextView
                android:id="@+id/textview_back1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="후면 상단"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_back1"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/back1_pic" />

            <TextView
                android:id="@+id/textview_back2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="후면"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/imageview_back2"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter"
                android:src="@drawable/back2_pic" />


        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>